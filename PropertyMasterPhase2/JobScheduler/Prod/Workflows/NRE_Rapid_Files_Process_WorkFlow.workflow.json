{
  "TYPE" : "Workflow",
  "version" : "1.6.6",
  "timeZone" : "America/New_York",
  "orderPreparation" : {
    "parameters" : {
      "checkDependency" : {
        "type" : "Boolean",
        "default" : "true"
      },
      "filePattern" : {
        "type" : "String",
        "default" : "'RAPID_GS_EXTRACT_[0-9][0-9][0-9][0-9][0-9][0-9][0-9][0-9]_[0-9][0-9][0-9][0-9][0-9][0-9][0-9][0-9]_[0-9][0-9][0-9][0-9][0-9][0-9].csv'"
      },
      "messageType" : {
        "type" : "String",
        "default" : "'NRE_BuildingMaster'"
      }
    },
    "allowUndeclared" : false
  },
  "instructions" : [ {
    "TYPE" : "If",
    "predicate" : "$checkDependency==true",
    "then" : {
      "instructions" : [ {
        "TYPE" : "Execute.Named",
        "jobName" : "Echo_Dependency_Check",
        "label" : "Echo_Dependency_Check"
      }, {
        "TYPE" : "Execute.Named",
        "jobName" : "JBLG_Check",
        "label" : "JBLG_Check"
      }, {
        "TYPE" : "If",
        "predicate" : "$count.toNumber == 0",
        "then" : {
          "instructions" : [ {
            "TYPE" : "Execute.Named",
            "jobName" : "NRE_Rapid_Files_Process_Job",
            "label" : "NRE_Rapid_Files_Process_Job"
          } ]
        }
      } ]
    },
    "else" : {
      "instructions" : [ {
        "TYPE" : "Execute.Named",
        "jobName" : "Echo_No_Dependency",
        "label" : "Echo_No_Dependency"
      }, {
        "TYPE" : "Execute.Named",
        "jobName" : "NRE_Rapid_Files_Process_Job_ND",
        "label" : "NRE_Rapid_Files_Process_Job_ND"
      } ]
    }
  } ],
  "jobs" : {
    "Echo_Dependency_Check" : {
      "agentName" : "JSAgent",
      "withSubagentClusterIdExpr" : false,
      "executable" : {
        "TYPE" : "ShellScriptExecutable",
        "script" : "echo \"dependency check\"",
        "v1Compatible" : false
      },
      "skipIfNoAdmissionForOrderDay" : false,
      "parallelism" : 1,
      "graceTimeout" : 1,
      "failOnErrWritten" : false,
      "warnOnErrWritten" : false
    },
    "JBLG_Check" : {
      "agentName" : "JSAgent",
      "withSubagentClusterIdExpr" : false,
      "executable" : {
        "TYPE" : "InternalExecutable",
        "className" : "com.sos.jitl.jobs.db.SQLExecutorJob",
        "internalType" : "JITL",
        "arguments" : {
          "command" : "\"select coalesce(sum(task_tot_cnt), 0) as count from ser_gs_gc.ft_t_jblg where job_input_txt ~ '$filePattern' and job_msg_typ = '$messageType' and date_trunc('day', ser_gs_gc.sysdate()) = date_trunc('day', job_start_tms) and job_end_tms is not null and job_stat_typ = 'CLOSED'\"",
          "resultset_as_variables" : "\"column_value\"",
          "hibernate_configuration_file" : "\"/ext/app/jobscheduler/base/sos-berlin.com/joc/resources/joc/hibernate.cfg.xml\""
        }
      },
      "skipIfNoAdmissionForOrderDay" : false,
      "parallelism" : 1,
      "graceTimeout" : 1,
      "failOnErrWritten" : false,
      "warnOnErrWritten" : false
    },
    "NRE_Rapid_Files_Process_Job" : {
      "agentName" : "JSAgent",
      "withSubagentClusterIdExpr" : false,
      "executable" : {
        "TYPE" : "ShellScriptExecutable",
        "script" : "cd $scripts_home\n./jobutilities execWkf $input_param_file $event_name $target_application",
        "env" : {
          "scripts_home" : "'/ext/app/jobscheduler/scripts/'",
          "input_param_file" : "'/edmfs/app/InputParams/NRE_Rapid_Files_Process.json'",
          "event_name" : "'NRE_EVT_S3BucketFilesProcess'",
          "target_application" : "'fileloading'"
        },
        "v1Compatible" : false
      },
      "skipIfNoAdmissionForOrderDay" : false,
      "parallelism" : 1,
      "graceTimeout" : 1,
      "failOnErrWritten" : false,
      "warnOnErrWritten" : false
    },
    "Echo_No_Dependency" : {
      "agentName" : "JSAgent",
      "withSubagentClusterIdExpr" : false,
      "executable" : {
        "TYPE" : "ShellScriptExecutable",
        "script" : "echo \"no dependency check\"",
        "v1Compatible" : false
      },
      "skipIfNoAdmissionForOrderDay" : false,
      "parallelism" : 1,
      "graceTimeout" : 1,
      "failOnErrWritten" : false,
      "warnOnErrWritten" : false
    },
    "NRE_Rapid_Files_Process_Job_ND" : {
      "agentName" : "JSAgent",
      "withSubagentClusterIdExpr" : false,
      "executable" : {
        "TYPE" : "ShellScriptExecutable",
        "script" : "cd $scripts_home\n./jobutilities execWkf $input_param_file $event_name $target_application",
        "env" : {
          "scripts_home" : "'/ext/app/jobscheduler/scripts/'",
          "input_param_file" : "'/edmfs/app/InputParams/NRE_Rapid_Files_Process.json'",
          "event_name" : "'NRE_EVT_S3BucketFilesProcess'",
          "target_application" : "'fileloading'"
        },
        "v1Compatible" : false
      },
      "skipIfNoAdmissionForOrderDay" : false,
      "parallelism" : 1,
      "graceTimeout" : 1,
      "failOnErrWritten" : false,
      "warnOnErrWritten" : false
    }
  }
}